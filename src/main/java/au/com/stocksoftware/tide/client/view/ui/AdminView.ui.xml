<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:style>
    .mainPanel {
      margin: 20px;
    }
  </ui:style>
  <g:HorizontalPanel styleName="{style.mainPanel}">
    <g:cell width="200px">
      <g:VerticalPanel ui:field="_userList" spacing="20">
      </g:VerticalPanel>
    </g:cell>
    <g:DecoratorPanel height="100%">
      <g:Label styleName="hspace">to</g:Label>
    </g:DecoratorPanel>
  </g:HorizontalPanel>
</ui:UiBinder>