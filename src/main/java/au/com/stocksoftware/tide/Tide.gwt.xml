<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='tide'>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.activity.Activity"/>
  <inherits name="com.google.gwt.place.Place"/>
  <inherits name="com.google.gwt.inject.Inject"/>
  <inherits name="com.google.gwt.logging.Logging"/>
  <!--<inherits name='com.bradrydzewski.gwt.calendar.Calendar'/>-->
  <!--<inherits name='com.bradrydzewski.gwt.calendar.theme.google.Google'/>-->
  <inherits name="org.realityforge.replicant.Replicant"/>

  <entry-point class='au.com.stocksoftware.tide.client.Tide'/>

  <source path='client'/>
  <source path='shared'/>

  <set-property name="gwt.logging.enabled" value="FALSE"/>

  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true"/>
</module>
